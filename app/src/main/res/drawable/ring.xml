<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp"
        android:left="4dp">
        <shape
            android:shape="oval">
            <size
                android:height="1dp"
                android:width="1dp"/>
            <solid android:color="#7EFFFFFF" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:width="1dp"
                android:color="#7AFFFFFF"/>
        </shape>
    </item>
</layer-list>
